void char2Arr(uint8_t ch, char PosX, short PosY, FontType fontType) 
{ //символы в arr
    uint8_t i, j, k, l, m, o1, o2, o3, o4;  //в LEDarr
    const unsigned char *font; // Указатель на текущий шрифт

    PosX++;
    k = ch - 32; // Позиция ASCII в шрифте
    if ((fontType == FONT3 && k >= 0 && k < 32) || 
        ((fontType == FONT1 || fontType == FONT2) && k >= 0 && k < 96)) { // Символ найден в шрифте?
        // Определяем шрифт на основе параметра fontType
        if (fontType == FONT3) {
            font = font3[k];
        } else if (fontType == FONT2) {
            font = font2[k];
        } else { // fontType == FONT1
            font = font1[k];
        }

        o4 = font[0]; // Ширина символа
        o3 = 1 << (o4 - 2); // Общий расчет для всех шрифтов
        for (i = 0; i < o4; i++) {
            if (PosX - i <= maxPosX && PosX - i >= 0 && PosY > -8 && PosY < 8) { // Внутри матрицы?
                o1 = helpArrPos[PosX - i];
                o2 = helpArrMAX[PosX - i];
                for (j = 0; j < 8; j++) {
                    if ((PosY >= 0 && PosY <= j) || (PosY < 0 && j < PosY + 8)) { // Прокрутка по вертикали
                        l = font[j + 1];
                        m = l & (o3 >> i);
                        if (m > 0)
                            LEDarr[o2][j - PosY] |= o1; // Установка точки
                        else
                            LEDarr[o2][j - PosY] &= ~o1; // Очистка точки
                    }
                }
            }
        }
    }
}

